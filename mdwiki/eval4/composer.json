{
  "name": "meebio/php-eval-console",
  "type": "library",
  "description": "Standalone php eval console. In-browser, standalone console that executes your PHP code and returns the produced output.",
  "keywords": [
    "eval",
    "console",
    "debug",
    "debugging",
    "sandbox"
  ],
  "homepage": "https://github.com/Meebio/php-eval-console",
  "license": "MIT",
  "authors": [
    {
      "name": "John Jablonski",
      "email": "jablonski.kce@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "fieryprophet/php-sandbox": "1.3.*"
  },
  "suggest": {
    "fieryprophet/php-sandbox": "Required for PhpSandboxEvaluator to work."
  },
  "autoload": {
    "psr-4": {
      "Meebio\\PhpEvalConsole\\": "src/"
    }
  }
}